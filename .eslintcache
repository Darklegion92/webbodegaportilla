   style={{ marginTop: '92px' }}\n            placement='right'\n            closable={false}\n            onClose={() => {\n              setVisibleRight(false)\n            }}\n            visible={visibleRight}\n            key='right'\n          >\n            <MenuCarrito carrito={carrito} cerrar={setVisibleRight} />\n          </Drawer>\n        </Col>\n      </Row>\n    )\n  }\n\n  /**Escritoio */\n  return (\n    <div className='headP'>\n      <Row align='middle' justify='center'>\n        <Col span={4} className='logo'>\n          <Link to='/'>\n            <img src='../img/logo.png' alt='logo.png' width='150px' />\n          </Link>\n        </Col>\n        <Col span={17}>\n          <Menu\n            onClick={handleClick}\n            selectedKeys={[current]}\n            mode='horizontal'\n          >\n            <Menu.Item key='inicio'>\n              <Title level={3} className='items' code={true}>\n                <Link to='/'>INICIO</Link>\n              </Title>\n            </Menu.Item>\n            <Menu.Item key='quienes'>\n              <Title level={3} className='items' code={true}>\n                <Link to='/shop'>TIENDA</Link>\n              </Title>\n            </Menu.Item>\n            <SubMenu\n              title={\n                <Title level={3} className='items' code={true}>\n                  <Link>PRODUCTOS</Link>\n                </Title>\n              }\n            >\n              {grupos.map(grupo => {\n                return (\n                  <SubMenu\n                    key={grupo.id}\n                    title={\n                      <Text style={{ color: 'var(--color-priamrio)' }}>\n                        {grupo.nombre}\n                      </Text>\n                    }\n                  >\n                    {grupo.subgrupos.map(subgrupo => (\n                      <Menu.Item key={subgrupo.id}>\n                        <div onClick={e=>onClickGrupo(grupo,subgrupo)}>\n                          <Link to='/shop