CO.URL + articulo.img)\n  const isTabletOrMobile = useMediaQuery({ maxWidth: 1224 })\n  const isTabletOrMobileDevice = useMediaQuery({ maxDeviceWidth: 1224 })\n\n  const onError = () => {\n    setImg('img/articulodefecto.png')\n  }\n\n  return (\n    <div className='articulo-comprado-tag'>\n      <Row className='fila1'>\n        <Text>TU PRODUCTO SE AGREGO AL CARRITO</Text>\n        <Divider />\n      </Row>\n      {isTabletOrMobile || isTabletOrMobileDevice ? (\n        <Col gutter={24}>\n          <Col span={24}>\n            <Row justify='center' style={{ height: '30%' }}>\n              <img\n                src={img}\n                width='70%'\n                onError={onError}\n                style={{ height: '100%' }}\n              />\n            </Row>\n          </Col>\n          <Col span={24}>\n            <Row className='fila2' justify='center'>\n              <Text align='center'>{articulo.nombre}</Text>\n            </Row>\n            <Row className='fila3' justify='center'>\n              <Text>\n              {numeral(articulo.descuento > 0\n                  ? Math.round(articulo.cantidad * articulo.descuento)\n                  : Math.round(articulo.precio * articulo.cantidad)).format('$ 0,0')}\n              </Text>\n            </Row>\n            <Row className='fila4' justify='center'>\n              <Link to='/carshop'>\n                <Button>IR AL CARRITO</Button>\n              </Link>\n              <Link to='/shop'>\n                <Button className='contrario' onClick={() => onOk(false)}>\n                  SEGUIR COMPRANDO\n                </Button>\n              </Link>\n            </Row>\n          </Col>\n        </Col>\n      ) : (\n        <>\n          <Row gutter={25}>\n            <Col span={8}>\n              <img src={img} width='100%' onError={onError} />\n            </Col>\n            <Col span={16}>\n              <Row align='middle' style={{ height: '100%' }}>\n                <Col>\n                  <Row clas